#include <iostream>
#include <cmath>

using namespace std;
int main()
{

	int n, i,ch_c = 0,ch_b = 0, k=0;
	cin >> n;
	int* m = new int[n];
	for (int i = 0; i < n; i++) 
	{
		cout << i << ": ";
		cin >> m[i];
	}
	for (int i = 0; i < n; i++) 
	{
		cout << m[i] << "\t";
	}
	cout << endl;
	int* b = new int[n];
	int* c = new int[n];

	b[k] = 1;
	c[k] = m[k];
	for (i = 1; i < n; i++) 
	{
		if (m[i] == m[i-1])
		{
			b[k]++;
		}
		else
		{
			k++;
			b[k] = 1;
			c[k] = m[i];
		}
	}

	cout << endl;
	for (i = 0;i <=k;i++)
	{
		cout<<b[i]<<"\t";
	}
	cout << endl;
	for (i = 0;i <=k; i++)
	{
		cout << c[i] << "\t";
	}

}
