#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	int n, i, s, ch,k;
	cin >> n;
	int* m = new int[n];
	int* m1 = new int[n-1];
	for (int i = 0; i < n; i++) {
		cout << i << ": ";
		cin >> m[i];
	}
	for (int i = 0; i < n; i++) {
		cout << m[i] << "\t";
	}

	cout << endl;
	cin >> s;
	k = 0;
	for (int i = 0; i < n; i++)
	{
		ch = 1;
		for (int j = i + 1; j < n; j++)
		{
			if (m[i] == m[j])
			{
				ch++;
			}
			else
			{
			break;
			}
		}
		if (ch > s)
		{
			m1[k] = 0;
			k++;
			i = i + ch - 1;
		}
		else 
		{
			m1[k] = m[i];
			k++;
		}
	}
	for (int i = 0; i < k;i++) 
	{
		cout << m1[i] << "\t";

	}

}
